/* components/tabNav/tabNav.wxss */
.tabNav{
  position: fixed;
  bottom: 0;
  display: flex;
  height: 100rpx;
  width: 100%;
  font-size: 22rpx;
  z-index:100;
  background: #fff;
  box-shadow: 0px -2px 6px 1px #eee;
}
.tabNav .active{
  color: #35BD64;
}
.tabNav > view{
  flex: 1;
  text-align: center;
  margin-top: 6rpx;
}
.tabNav image{
  width: 50rpx;
  height: 50rpx;
}
.addNote{
  width: 90rpx;
  height: 90rpx;
  margin: -46rpx auto 0;
  line-height: 70rpx;
  font-size: 70rpx;
  color: #fff;
  background: #35BD64;
  border-radius: 50%;
  box-shadow: 0px 0px 8px 1px #87e6a6;
  display: flex;
  align-items: center;
  justify-content: center;
}
.addNote image{
  width: 47rpx;
  height: 64rpx;
}